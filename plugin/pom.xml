<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>consulo</groupId>
        <artifactId>arch.ide-api-provided</artifactId>
        <version>3-SNAPSHOT</version>
        <relativePath/>
    </parent>

    <groupId>consulo.plugin</groupId>
    <artifactId>consulo.java</artifactId>
    <version>3-SNAPSHOT</version>
    <packaging>consulo-plugin</packaging>

    <repositories>
        <repository>
            <id>consulo</id>
            <url>https://maven.consulo.io/repository/snapshots/</url>
            <snapshots>
                <enabled>true</enabled>
                <updatePolicy>interval:60</updatePolicy>
            </snapshots>
        </repository>
    </repositories>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <compilerArguments>
                        <Xmaxerrs>5000</Xmaxerrs>
                    </compilerArguments>
                </configuration>
            </plugin>
            <plugin>
                <groupId>consulo.maven</groupId>
                <artifactId>maven-consulo-plugin</artifactId>
                <extensions>true</extensions>
                <configuration>
                    <dependencies>
                        <dependency>com.intellij.spellchecker</dependency>
                        <dependency>com.intellij.regexp</dependency>
                        <dependency>com.intellij.xml</dependency>
                        <dependency>consulo.library.batik</dependency>
                        <dependency>com.intellij.images</dependency>
                        <dependency>com.intellij.properties</dependency>
                        <dependency>consulo.javascript</dependency>
                    </dependencies>
                    <packaging>
                        <skip>${dev.mode}</skip>
                        <version>${build.number}</version>
                        <copies>
                            <copy>
                                <artifact>${project.groupId}:consulo.java-jdk.annotations:${project.version}</artifact>
                                <path>jdk-annotations.jar</path>
                            </copy>
                            <copy>
                                <artifact>${project.groupId}:consulo.java-java.rt.shaded:${project.version}</artifact>
                                <path>java-rt-shaded.jar</path>
                            </copy>
                            <copy>
                                <artifact>${project.groupId}:consulo.java-java.rt:${project.version}</artifact>
                                <path>java-rt.jar</path>
                            </copy>
                            <copy>
                                <artifact>consulo.internal:java.coverage.agent:2.0</artifact>
                                <path>coverage/consulo/agent.jar</path>
                            </copy>
                        </copies>
                    </packaging>
                </configuration>
                <executions>
                    <execution>
                        <id>package</id>
                        <phase>package</phase>
                        <goals>
                            <goal>workspace</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>generate-sources</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>generate-localize</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

